{
  "python.pythonPath": "${workspaceFolder}/venv/bin/python",
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pycodestyleEnabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--max-line-length=120"],
  "python.testing.pytestEnabled": true,
  "python.testing.pytestArgs": ["tests"],
  "python.autoComplete.addBrackets": true,
  "python.analysis.typeCheckingMode": "off",
  "python.analysis.autoSearchPaths": true,
  "python.analysis.memory.keepLibraryAst": true,
  "python.analysis.memory.keepLibraryLocalVariables": true,
  "python.analysis.memory.keepLibraryConstants": true,
  "python.analysis.memory.keepLibraryClasses": true,
  "python.analysis.memory.keepLibraryFunctionDefs": true,
  "python.analysis.memory.keepLibraryImports": true,
  "python.analysis.memory.keepLibraryVariables": true,
  "python.analysis.memory.keepLibraryDecorators": true,
  "python.analysis.memory.keepLibraryTypeAliases": true,
  "python.analysis.memory.keepLibraryNamedExpressions": true,
  "python.analysis.memory.keepLibraryOperators": true,
  "python.analysis.memory.keepLibraryControlFlowBlocks": true,
  "python.analysis.memory.keepLibraryYieldExpressions": true,
  "python.analysis.memory.keepLibraryStringLiterals": true,
  "python.analysis.memory.keepLibraryDocStrings": true,
  "python.analysis.memory.keepLibraryComments": true,
  "python.analysis.memory.keepLibraryEmptyLines": true,
  "python.analysis.memory.keepLibraryWhitespace": true,
  "python.analysis.memory.keepLibraryAll": true,
  "python.analysis.memory.keepLibrary": ["fastapi", "uvicorn", "jsonrpcserver"],
  "python.analysis.completeFunctionParens": true
}
